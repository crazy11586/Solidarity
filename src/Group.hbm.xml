<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
      
<hibernate-mapping package="com.jsu.po">

	<class name="Group" table="friend_group" catalog="conveniently">

		<id name="group_id" column="group_id" type="string">
		</id>

		<property name="group_name" column="group_name" type="string"
			length="255" unique="false" />
			
		<property name="bellow_uid" column="bellow_uid" type="string"
			length="255" unique="false" />
	</class>
	
</hibernate-mapping>